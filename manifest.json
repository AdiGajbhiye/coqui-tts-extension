{
  "manifest_version": 2,
  "name": "COQUI-TTS",
  "version": "1.0",
  "permissions": ["tabs", "activeTab", "tts"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": {}
  }
}
